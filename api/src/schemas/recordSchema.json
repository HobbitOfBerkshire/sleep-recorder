{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "maxLength": 12
    },
    "gender": {
      "type": "string",
      "enum": ["Male", "Female"]
    },
    "duration": {
      "type": "number",
      "minimum": 0,
      "maximum": 24
    },
    "date": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    }
  },
  "required": ["name", "gender", "duration", "date"]
}
